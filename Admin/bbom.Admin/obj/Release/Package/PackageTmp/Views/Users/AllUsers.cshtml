@model IQueryable<bbom.Data.Model.AspNetUser>

@{
    ViewBag.Title = "AllUsers";
}

<h2>@ViewBag.Title</h2>
<div class="panel panel-default" style="margin-top: 20px">
    <div class="panel panel-heading">Список</div>
    <div class="panel-body">
        @foreach (var user in Model)
        {
            <strong>@user.UserName (@user.GetFio())</strong>
            <a href="@Url.Action("UserMenager", "Users", new {id = user.UserName})">info</a>
            <br/>
        }
    </div>
</div>

